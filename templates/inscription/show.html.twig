{% extends 'base.html.twig' %}

{% block title %}Inscription{% endblock %}

{% block body %}
    <h1>Inscription</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ inscription.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ inscription.nom }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ inscription.email }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ inscription.telephone }}</td>
            </tr>
            <tr>
                <th>NombrePlaces</th>
                <td>{{ inscription.nombrePlaces }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ inscription.createdAt ? inscription.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_inscription_index') }}">back to list</a>

    <a href="{{ path('app_inscription_edit', {'id': inscription.id}) }}">edit</a>

    {{ include('inscription/_delete_form.html.twig') }}
{% endblock %}
